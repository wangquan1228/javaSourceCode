<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<!-- 配置表与实体对象的关系 -->
<hibernate-mapping package="com.wq.entity">
<!-- class name="实体类名" table="数据库表名" -->
	<class name="SysUser" table="sysuser">
		<id name="userId" column="userId">
			<!-- generator:主键生成策略 -->
			<generator class="native"></generator>
		</id>
		<property name="userName" column="userName"></property>
		<property name="userCode" column="userCode"></property>
		<property name="userPassword" column="userPassword"></property>
		<property name="userState" column="userState"></property>
		 
	</class>
</hibernate-mapping>